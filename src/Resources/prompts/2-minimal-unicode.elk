fn elkPrompt() {
    let pwdOptions = {
        "wholeFolderNameAmount": 2,
    }
    {{ GIT_BRANCH_VAR }}
    let sections = [
        env::prettyPwd(pwdOptions) | ansi::color("blue"),
        {{ GIT_BRANCH_SECTION }}
        ansi::color(" (venv)", "green") if $VIRTUAL_ENV,
        " â¯ ",
    ]

    sections
        | map => x { x ?? "" }
        | join
}