fn sel(container) => closure {
    let values = []
    for value in container: values | add(closure(value))

    values
}

let ending = "!"
let result = ["this", "is", "a", "test"]
    | sel => x: x + ending
assert(result[0] == "this!")
assert(result[3] == "test!")

let ints = ["1", "2"] | sel => &int
assert(ints[0] == 1)
assert(ints[1] == 2)

fn thing() => closure {
    closure()
}

thing =>: assert(true)