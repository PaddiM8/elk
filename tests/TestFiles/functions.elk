fn confirmParamCount(expectedCount, params...) {
    assert(expectedCount == len(params))
}

confirmParamCount(1, "a")
confirmParamCount(2, "a", "b")

assert((simple test) == "test")
assert(simple("test") == "test")

fn simple(a) {
    a
}

assert((empty) == "empty")
assert(empty() == "empty")

fn empty() {
    "empty"
}

# recursion
fn rec(x) {
    if x > 0: rec(x - 1) else x
}

assert(rec(10) == 0)